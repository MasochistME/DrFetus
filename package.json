{
  "name": "drfetus",
  "version": "3.0.0",
  "description": "A custom Dr. Fetus Bot for the 0.1% community!",
  "main": "fetus.js",
  "scripts": {
    "build": "env NODE_PATH=./src tsc -p .",
    "start": "env NODE_PATH=./src ts-node dist/src/fetus.js",
    "start-dev": "env NODE_PATH=./src ts-node src/fetus.ts",
    "fetus": "npm run build && npm run start",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PointOnePercent/DrFetus.git"
  },
  "author": "Arcyvilk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PointOnePercent/DrFetus/issues"
  },
  "homepage": "https://github.com/PointOnePercent/DrFetus#readme",
  "dependencies": {
    "axios": ">=0.21.1",
    "compromise": "^12.2.1",
    "discord.js": "^12.5.1",
    "dotenv": "^10.0.0",
    "mongodb": "^3.2.3",
    "rimraf": "^2.6.3",
    "tsc": "^1.20150623.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^11.15.43",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lodash": "^4.17.19",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
